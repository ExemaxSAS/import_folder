<?xml version="1.0" encoding="UTF-8"?>
<odoo>
    <!--MODELO PROJECT PROJECT-->
    <record model="ir.ui.view" id="project_importation_form_view">
        <field name="name">project.importation.form</field>
        <field name="model">project.project</field>
        <field name="inherit_id" ref="project.edit_project"/>
        <field name="arch" type="xml">
            <xpath expr="//field[@name='tag_ids']" position="after">
                <field name="importation" groups="import_folder.group_project_importation_manager"/>
            </xpath>
        </field>
    </record>
    
</odoo>